        </array>
        <key>firstLineMatch</key>
        <string>(?i)-[*]-\s*(mode:)?\s*odin\s*-[*]-</string>
        <key>uuid</key>
        <string>d94cbd06-b698-4935-b65c-501eb3e1e0b0</string>
        <key>patterns</key>
        <array>
            <dict>
                <key>include</key>
                <string>#comments</string>
            </dict>
            <dict>
                <key>include</key>
                <string>#types</string>
            </dict>
            <dict>
                <key>include</key>
                <string>#keywords</string>
            </dict>
            <dict>
                <key>include</key>
                <string>#functions_and_declarations</string>
            </dict>
            <dict>
                <key>include</key>
                <string>#strings</string>
            </dict>
            <dict>
                <key>include</key>
                <string>#string_escaped_char</string>
